	<queryString>
		<![CDATA[SELECT
     comp.`codigo`,
     comp.`nome`,
     mo.`nome` nomemodelo,
     comp.`processador`,
     comp.`memoria`,
     comp.`hd`,
     comp.`sistema_operacional`
FROM
     `equipamento` eq INNER JOIN `computador` comp ON eq.`codigo` = comp.`codigo`
     INNER JOIN `modelo` mo ON eq.`codigo_modelo` = mo.`codigo`
ORDER BY
     comp.nome ASC]]>
